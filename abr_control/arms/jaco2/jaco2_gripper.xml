<!-- Download stl files from -->
<!-- https://github.com/Kinovarobotics/kinova-ros/kinova_description/meshes -->
<!-- and place them in a meshes folder in this directory -->
<mujoco model="jaco2_gripper">
    <!-- <option solver="CG"/> -->
    <!--  -->
    <compiler angle="radian" meshdir="meshes" balanceinertia="true"/>

    <default>
        <light castshadow="false" diffuse="1 1 1" />
        <camera fovy="60" />
    </default>

    <custom>
        <numeric name="START_ANGLES" data="2.0 3.14 1.57 4.71 0.0 3.04"/>
    </custom>

    <asset>
        <mesh file="base.STL" />
        <mesh file="shoulder.STL"/>
        <mesh file="arm.STL"/>
        <mesh file="forearm.STL"/>
        <mesh file="wrist.STL"/>
        <mesh file="hand_3finger.STL"/>
        <mesh file="ring_big.STL"/>
        <mesh file="ring_small.STL"/>
        <mesh file="finger_distal.STL"/>
        <mesh file="finger_proximal.STL"/>
        <mesh file="octagon.stl"/>
        <mesh file="adapt.stl" scale="0.5 0.5 0.5"/>
        <mesh file="brain.stl"/>

        <texture name="mars_terrain" file="meshes/mars_terrain.png" width="2000" height="2000" type="2d"/>
        <material name="mars_terrain" texture="mars_terrain" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="earth_terrain" file="meshes/earth_terrain.png" width="2000" height="2000" type="2d"/>
        <material name="earth_terrain" texture="earth_terrain" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="moon_terrain" file="meshes/moon_terrain.png" width="1200" height="1200" type="2d"/>
        <material name="moon_terrain" texture="moon_terrain" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="jupiter_terrain" file="meshes/jupiter_terrain.png" width="2000" height="2000" type="2d"/>
        <material name="jupiter_terrain" texture="jupiter_terrain" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="floor_tile" file="meshes/floor_tile.png" width="3000" height="3000" type="2d"/>
        <material name="tile" texture="floor_tile" specular="0" shininess="0" reflectance="0" emission="1" />


        <material name="blue_steel" specular="1" shininess="1" reflectance="1" rgba="0.1 0.125 0.15 1" emission="1" />
        <material name="carbon" specular="1" shininess="1" reflectance="1" rgba="0.05 0.05 0.05 1" emission="1" />

        <material name="grey_plastic" specular="0.5" shininess="0" reflectance="0" rgba="0.12 0.14 0.14 1" emission="1" />

        <texture name="abr_logo" file="meshes/abr_logo.png" width="193" height="193" type="2d"/>
        <material name="logo" texture="abr_logo"/>

        <texture name="abr_logo_flip" file="meshes/abr_logo.png" width="193" height="193" type="2d" hflip="true"/>
        <material name="logo_flip" texture="abr_logo_flip"/>

        <texture name="foam_blocks" file="meshes/foam_blocks.png" width="2744" height="2744" type="2d"/>
        <material name="foam" texture="foam_blocks" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="knurled_texture" file="meshes/grip.png" width="600" height="600" type="cube"/>
        <material name="knurled" texture="knurled_texture" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="mars" file="meshes/mars_crop.png" width="882" height="882" type="2d"/>
        <material name="mars_surface" texture="mars" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="jupiter" file="meshes/jupiter_crop.png" width="2048" height="2048" type="2d"/>
        <material name="jupiter_surface" texture="jupiter" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="moon" file="meshes/moon_crop.png" width="2048" height="2048" type="2d"/>
        <material name="moon_surface" texture="moon" specular="0" shininess="0" reflectance="0" emission="1" />

        <texture name="earth" file="meshes/earth3x4.png" width="2048" height="1536"
            gridlayout=".U..LFRB.D.." gridsize="3 4"/>
        <!-- <texture name="earth" file="meshes/earth_crop.png" width="2048" height="2048" type="2d"/> -->
        <material name="earth_surface" texture="earth" specular="0" shininess="0" reflectance="0" emission="1" />


        <!-- <texture name="stars" file="meshes/stars_scale.png" width="2048" height="2048" type="skybox"/> -->
        <!--  -->
        <texture name="stars" file="meshes/stars3x4.png" width="2048" height="2048" type="skybox"
            gridlayout=".U..LFRB.D.." gridsize="3 4"/>
    </asset>

