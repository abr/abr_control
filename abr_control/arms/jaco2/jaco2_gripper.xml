    <!-- <option solver="PGS"/> -->
    <!--  -->
    <compiler angle="radian" meshdir="meshes" balanceinertia="true"/>

    <default>
        <light castshadow="false" diffuse="1 1 1" />
        <camera fovy="60" />
    </default>

    <custom>
        <numeric name="START_ANGLES" data="0.0 2.79 2.62 4.71 0.0 3.04"/>
        <numeric name="N_JOINTS" data="6"/>
