        <body name="earth_floor" mocap="true">
            <geom name="earth_floor" pos="0 0 0.0" size="1 0.01"
                type="cylinder" material="earth_terrain"/>
        </body>

        <body name="mars_floor" mocap="true">
            <geom name="mars_floor" pos="0 0 0.0" size="1 0.01"
                type="cylinder" material="mars_terrain"/>
        </body>

        <body name="moon_floor" mocap="true">
            <geom name="moon_floor" pos="0 0 0.0" size="1 0.01"
                type="cylinder" material="moon_terrain"/>
        </body>

        <body name="jupiter_floor" mocap="true">
            <geom name="jupiter_floor" pos="0 0 0.0" size="1 0.01"
                type="cylinder" material="jupiter_terrain"/>
        </body>

        <body name="ISS_floor" mocap="true">
            <geom name="ISS_floor" pos="0 0 0.0" size="1 0.01"
                type="cylinder" material="tile"/>
        </body>

        <body name="hand" pos="0 0 -0.5" mocap="true">
            <geom type="box" size=".002 .004 .006" rgba="0 .9 0 .5" contype="2"/>
        </body>

        <body name="target" pos="0 0 -0.5" mocap="true">
            <geom name="target" type="sphere" size="0.05" rgba=".9 0 0 .5"/>
        </body>

        <body name="target_orientation" pos="0 0 -0.5" mocap="true">
            <geom type="box" size=".02 .04 .06" rgba=".9 0 0 .5" contype="2"/>
        </body>

        <body name="path_planner" pos="0 0 -0.5" mocap="true">
            <geom type="sphere" size="0.05" rgba="0 1 1 0.5"/>
        </body>

        <body name="path_planner_orientation" pos="0 0 -0.5" mocap="true">
            <geom type="box" size=".02 .04 .06" rgba="0 1 1 .5" contype="2"/>
        </body>

        <body name="obstacle" pos="0 0 -0.5" mocap="true">
            <geom type="sphere" size="0.05" rgba="0 0 1 1"/>
        </body>

        <body name="elbow" pos="0 0 -0.5" mocap="true">
            <geom name="elbow" type="sphere" size="0.05" rgba="0 1 1 .25"/>
        </body>

        <light directional="true" pos="-0.5 0.5 3" dir="0 0 -1" />

        <body name="dumbbell_stand1" pos="-0.39 0 0.015">
            <geom name="dumbbell_stand1" type="box" size="0.05 0.075 0.015"
                friction="1 0.005 0.0001" solimp="0.95 0.95 0.01" solref="0.01 1"
                condim="4" material='knurled'/>
        </body>
        <body name="dumbbell_stand2" pos="-0.61 0 0.015">
            <geom name="dumbbell_stand2" type="box" size="0.05 0.075 0.015"
                friction="1 0.005 0.0001" solimp="0.95 0.95 0.01" solref="0.01 1"
                condim="4" material='knurled'/>
        </body>

        <body name="dumbbell" pos="-0.5 0 0.085">
            <geom name="handle" type="cylinder" pos="0 0 0" material="knurled" size="0.0127 0.06" euler="0 1.57 0"
                friction="1 0.5 0.01" solimp="0.95 0.95 0.01 0.5 6" solref="0.01 1" condim="6" margin="0.0"/>
            <geom name="mass1" type="mesh" mesh="octagon" pos="0.1 0 0" material="carbon"
                friction="1 0.005 0.001" solimp="0.95 0.95 0.01" solref="0.01 1" condim="6" />
            <geom name="mass2" type="mesh" mesh="octagon" pos="-0.1 0 0" material="carbon"
                friction="1 0.005 0.001" solimp="0.95 0.95 0.01" solref="0.01 1" condim="6" />
            <inertial pos="0 0 0" mass="4" diaginertia="0.1 0.1 0.1"/>
            <freejoint name="dumbbell"/>
        </body>

        <body name="adapt" pos="0 1 0" mocap="true">
            <geom name="adapt" type="mesh" mesh="adapt" rgba="0.5 0.5 0.5 0.1"
                material='knurled'/>
        </body>

        <body name="brain" pos="0 0 1" mocap="true">
            <geom type="mesh" mesh="brain" rgba="0.92 0.82 0.84 1"/>
        </body>

    <body name="ISS" pos="1.0 1.0 -1" mocap="true">
        <geom name="ISS" type="sphere" size="0.0005"/>
